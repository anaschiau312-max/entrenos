      ".read": "auth != null",
      ".write": "root.child('users').child(auth.uid).child('role').val() === 'coach'"
    },
    "workoutLogs": {
      "$uid": {
        ".read": "$uid === auth.uid || root.child('users').child(auth.uid).child('role').val() === 'coach'",
        ".write": "$uid === auth.uid"
      }
    },
    "phases": {
      ".read": "auth != null",
      ".write": "root.child('users').child(auth.uid).child('role').val() === 'coach'"
    }
  }
